class Math {

        /* Returns a%b */
        class mod(a, b)
        {
            return a-(int(a/b)*b);
        }

        /* Returns a^b for positive b */
        class pow(a, b)
        {
            var res = 1;
            for (var i = 0; i < b; i=i+1) {
                res = res*a;
            }
            return res;
        }
}
